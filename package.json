    "zod": "^3.11.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "start:tailwindcss": "craco start",
    "build:tailwindcss": "craco build",
    "test:tailwindcss": "craco test",
    "eject": "react-scripts eject",
    "i18next-extract": "NODE_ENV=development babel './src/components/**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "locales": [
            "ja",
            "en"
          ],
          "outputPath": "./src/locales/{{locale}}.json"
        }
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/draft-js": "^0.11.3",
    "@types/jsdom": "^16.2.13",
    "@types/styled-components": "^5.1.9",
    "babel-jest": "^27.4.5",
    "babel-plugin-i18next-extract": "^0.8.3",
    "es6-shim": "^0.35.6",
    "jest": "^27.0.6",
    "jsdom": "^16.6.0",
    "sass": "^1.43.2",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.3"
  }
}

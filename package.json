    "react-i18next": "^11.12.0",
    "react-indiana-drag-scroll": "^2.1.0",
    "react-resizable": "^3.0.4",
    "react-rnd": "^10.3.5",
    "react-scripts": "4.0.3",
    "react-use": "^17.3.1",
    "styled-components": "^5.2.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
